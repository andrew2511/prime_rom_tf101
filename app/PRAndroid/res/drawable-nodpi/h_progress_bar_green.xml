<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ff3e3e3e" android:endColor="#ff585858" android:angle="90.0" />
            <stroke android:width="1.0px" android:color="#ff252525" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#80ffd300" android:endColor="#a0ffcb00" android:angle="270.0" android:centerY="0.75" android:centerColor="#80ffb600" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ff4bae4b" android:endColor="#ffceee6b" android:angle="90.0" android:centerY="0.75" android:centerColor="#ffa8d045" />
                <stroke android:width="1.0px" android:color="#ff107311" />
            </shape>
        </clip>
    </item>
</layer-list>
