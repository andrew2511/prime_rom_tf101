<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:colorScheme="white">
            <key-style latin:isModifier="true" latin:styleName="functionalKeyStyle" />
        </case>
        <case latin:colorScheme="black">
            <key-style latin:styleName="functionalKeyStyle" />
        </case>
    </switch>
    <switch>
        <case latin:clobberSettingsKey="true">
            <key-style latin:styleName="settingsPopupStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
        <default>
            <key-style latin:popupCharacters="\@drawable/sym_keyboard_settings_holo|@integer/key_settings" latin:keyHintIcon="@drawable/hint_popup_holo" latin:styleName="settingsPopupStyle" latin:parentStyle="functionalKeyStyle" />
        </default>
    </switch>
    <switch>
        <case latin:colorScheme="white">
            <key-style latin:code="@integer/key_shift" latin:isSticky="true" latin:iconPreview="@drawable/sym_keyboard_feedback_shift" latin:keyIcon="@drawable/sym_keyboard_shift_holo" latin:shiftedIcon="@drawable/sym_keyboard_shift_locked_holo" latin:styleName="shiftKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_delete" latin:isRepeatable="true" latin:iconPreview="@drawable/sym_keyboard_feedback_delete" latin:keyIcon="@drawable/sym_keyboard_delete_holo" latin:styleName="deleteKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_return" latin:iconPreview="@drawable/sym_keyboard_feedback_return" latin:keyIcon="@drawable/sym_keyboard_return_holo" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_space" latin:iconPreview="@drawable/sym_keyboard_feedback_space" latin:styleName="spaceKeyStyle" />
            <key-style latin:code="@integer/key_space" latin:iconPreview="@drawable/sym_keyboard_feedback_space" latin:styleName="nonSpecialBackgroundSpaceKeyStyle" />
            <key-style latin:popupCharacters="@string/alternates_for_smiley" latin:maxPopupKeyboardColumn="5" latin:keyOutputText=":-) " latin:keyLabel=":-)" latin:keyHintIcon="@drawable/hint_popup_holo" latin:styleName="smileyKeyStyle" />
            <key-style latin:code="@integer/key_voice" latin:iconPreview="@drawable/sym_keyboard_feedback_mic" latin:keyIcon="@drawable/sym_keyboard_voice_holo" latin:styleName="micKeyStyle" latin:parentStyle="settingsPopupStyle" />
            <key-style latin:code="@integer/key_settings" latin:iconPreview="@drawable/sym_keyboard_feedback_settings" latin:keyIcon="@drawable/sym_keyboard_settings_holo" latin:styleName="settingsKeyStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
        <case latin:colorScheme="black">
            <key-style latin:code="@integer/key_shift" latin:isSticky="true" latin:iconPreview="@drawable/sym_keyboard_feedback_shift" latin:keyIcon="@drawable/sym_bkeyboard_shift" latin:shiftedIcon="@drawable/sym_bkeyboard_shift_locked" latin:styleName="shiftKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_delete" latin:isRepeatable="true" latin:iconPreview="@drawable/sym_keyboard_feedback_delete" latin:keyIcon="@drawable/sym_bkeyboard_delete" latin:styleName="deleteKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_return" latin:iconPreview="@drawable/sym_keyboard_feedback_return" latin:keyIcon="@drawable/sym_bkeyboard_return" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_space" latin:iconPreview="@drawable/sym_keyboard_feedback_space" latin:styleName="spaceKeyStyle" />
            <key-style latin:code="@integer/key_space" latin:iconPreview="@drawable/sym_keyboard_feedback_space" latin:styleName="nonSpecialBackgroundSpaceKeyStyle" />
            <key-style latin:popupCharacters="@string/alternates_for_smiley" latin:maxPopupKeyboardColumn="5" latin:keyOutputText=":-) " latin:keyLabel=":-)" latin:keyHintIcon="@drawable/hint_popup_holo" latin:styleName="smileyKeyStyle" />
            <key-style latin:code="@integer/key_voice" latin:iconPreview="@drawable/sym_keyboard_feedback_mic" latin:keyIcon="@drawable/sym_bkeyboard_mic" latin:styleName="micKeyStyle" latin:parentStyle="settingsPopupStyle" />
            <key-style latin:code="@integer/key_settings" latin:iconPreview="@drawable/sym_keyboard_feedback_settings" latin:keyIcon="@drawable/sym_bkeyboard_settings" latin:styleName="settingsKeyStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
    </switch>
    <key-style latin:code="@integer/key_tab" latin:iconPreview="@drawable/sym_keyboard_tab_holo" latin:keyIcon="@drawable/sym_keyboard_tab_holo" latin:styleName="tabKeyStyle" latin:parentStyle="functionalKeyStyle" />
    <key-style latin:code="@integer/key_switch_alpha_symbol" latin:keyLabel="@string/label_to_symbol_key" latin:keyLabelOption="fontNormal" latin:styleName="toSymbolKeyStyle" latin:parentStyle="functionalKeyStyle" />
    <key-style latin:code="@integer/key_switch_alpha_symbol" latin:keyLabel="@string/label_to_alpha_key" latin:keyLabelOption="fontNormal" latin:styleName="toAlphaKeyStyle" latin:parentStyle="functionalKeyStyle" />
    <key-style latin:code="@integer/key_shift" latin:isSticky="true" latin:keyLabel="@string/label_more_key" latin:keyLabelOption="fontNormal" latin:styleName="moreKeyStyle" latin:parentStyle="functionalKeyStyle" />
    <key-style latin:popupCharacters="@string/alternates_for_popular_domain" latin:keyOutputText="@string/keylabel_for_popular_domain" latin:keyLabel="@string/keylabel_for_popular_domain" latin:keyLabelOption="fontNormal" latin:keyHintIcon="@drawable/hint_popup_holo" latin:styleName="comKeyStyle" />
    <switch>
        <case latin:passwordInput="true">
            <key-style latin:enabled="false" latin:styleName="nonPasswordSymbolKeyStyle" />
        </case>
        <default>
            <key-style latin:enabled="true" latin:styleName="nonPasswordSymbolKeyStyle" />
        </default>
    </switch>
</merge>
