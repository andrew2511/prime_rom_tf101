<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/kbd_key_styles" />
    <Row latin:keyWidth="10.000002%p" latin:rowEdgeFlags="top">
        <Key latin:popupCharacters="@string/alternates_for_q" latin:keyEdgeFlags="left" latin:keyLabel="q" latin:keyHintIcon="@drawable/key_hint_num1" />
        <Key latin:popupCharacters="@string/alternates_for_w" latin:keyLabel="w" latin:keyHintIcon="@drawable/key_hint_num2" />
        <Key latin:popupCharacters="@string/alternates_for_e" latin:keyLabel="e" latin:keyHintIcon="@drawable/key_hint_num3" />
        <Key latin:popupCharacters="@string/alternates_for_r" latin:keyLabel="r" latin:keyHintIcon="@drawable/key_hint_num4" />
        <Key latin:popupCharacters="@string/alternates_for_t" latin:keyLabel="t" latin:keyHintIcon="@drawable/key_hint_num5" />
        <Key latin:popupCharacters="@string/alternates_for_z" latin:keyLabel="z" latin:keyHintIcon="@drawable/key_hint_num6" />
        <Key latin:popupCharacters="@string/alternates_for_u" latin:keyLabel="u" latin:keyHintIcon="@drawable/key_hint_num7" />
        <Key latin:popupCharacters="@string/alternates_for_i" latin:keyLabel="i" latin:keyHintIcon="@drawable/key_hint_num8" />
        <Key latin:popupCharacters="@string/alternates_for_o" latin:keyLabel="o" latin:keyHintIcon="@drawable/key_hint_num9" />
        <Key latin:keyWidth="0.0%p" latin:popupCharacters="@string/alternates_for_p" latin:keyEdgeFlags="right" latin:keyLabel="p" latin:keyHintIcon="@drawable/key_hint_num0" />
    </Row>
    <include latin:keyboardLayout="@xml/kbd_qwerty_row2" />
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyWidth="14.999998%p" latin:keyEdgeFlags="left" latin:keyStyle="shiftKeyStyle" />
        <Key latin:popupCharacters="@string/alternates_for_y" latin:keyLabel="y" />
        <Key latin:keyLabel="x" />
        <Key latin:popupCharacters="@string/alternates_for_c" latin:keyLabel="c" />
        <Key latin:popupCharacters="@string/alternates_for_v" latin:keyLabel="v" />
        <Key latin:keyLabel="b" />
        <Key latin:popupCharacters="@string/alternates_for_n" latin:keyLabel="n" />
        <Key latin:keyLabel="m" />
        <Key latin:keyWidth="0.0%p" latin:keyEdgeFlags="right" latin:keyStyle="deleteKeyStyle" />
    </Row>
    <include latin:keyboardLayout="@xml/kbd_qwerty_row4" />
</merge>
