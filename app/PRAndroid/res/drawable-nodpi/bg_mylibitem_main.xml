<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff2260cd" android:endColor="#ff5b99e6" android:angle="90.0" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <gradient android:startColor="#ff2260cd" android:endColor="#ff5b99e6" android:angle="90.0" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff4a4a4a" android:endColor="#ff656565" android:angle="90.0" />
        </shape>
    </item>
</selector>
