<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="watch_video_button">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/btn_video_link</item>
        <item name="android:paddingRight">16.0dip</item>
    </style>
    <style name="help_dialog_content">
        <item name="android:background">@drawable/vs_editordialog_bubble</item>
    </style>
    <style name="Theme.Recognition" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@color/translucent_black</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="Theme.VoiceSearchDialog" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="help_dialog_button_left">
        <item name="android:background">@drawable/btn_top_left</item>
    </style>
    <style name="help_dialog_button_right">
        <item name="android:background">@drawable/btn_top_right</item>
    </style>
    <style name="square_button">
        <item name="android:background">@drawable/btn_square</item>
    </style>
    <style name="disambig_dialog_content">
        <item name="android:background">@drawable/vs_dialog_bubble</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="Theme.Suggestion" parent="@android:style/Theme.NoTitleBar">
        <item name="android:colorBackground">#ffffffff</item>
        <item name="android:windowBackground">@color/white</item>
    </style>
    <style name="correctable_highlight" parent="@android:style/TextAppearance">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/correctable_highlight_color</item>
    </style>
    <style name="contacts_list_main" parent="@android:style/TextAppearance">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/editor_grayed_out_text</item>
    </style>
    <style name="contacts_list_highlight" parent="@android:style/TextAppearance">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="slot_title_main" parent="@android:style/TextAppearance">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="slot_title_invalid" parent="@android:style/TextAppearance">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffcc0000</item>
    </style>
    <style name="positioned_dialog">
        <item name="android:layout_gravity">top|center</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="help_dialog" parent="@style/positioned_dialog">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/help_dialog_marginTop</item>
    </style>
    <style name="disambig_dialog" parent="@style/positioned_dialog">
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>
