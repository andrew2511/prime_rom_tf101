<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:colorScheme="white">
            <key-style latin:isModifier="true" latin:styleName="functionalKeyStyle" />
        </case>
        <case latin:colorScheme="black">
            <key-style latin:styleName="functionalKeyStyle" />
        </case>
    </switch>
    <switch>
        <case latin:clobberSettingsKey="true">
            <key-style latin:styleName="settingsPopupStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
        <default>
            <key-style latin:popupCharacters="\@drawable/sym_keyboard_settings_holo|@integer/key_settings" latin:keyHintIcon="@drawable/hint_popup" latin:styleName="settingsPopupStyle" latin:parentStyle="functionalKeyStyle" />
        </default>
    </switch>
    <switch>
        <case latin:colorScheme="white">
            <key-style latin:code="@integer/key_shift" latin:isSticky="true" latin:iconPreview="@drawable/sym_keyboard_feedback_shift" latin:keyIcon="@drawable/sym_keyboard_shift" latin:shiftedIcon="@drawable/sym_keyboard_shift_locked" latin:styleName="shiftKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_delete" latin:isRepeatable="true" latin:iconPreview="@drawable/sym_keyboard_feedback_delete" latin:keyIcon="@drawable/sym_keyboard_delete" latin:styleName="deleteKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <switch>
                <case latin:voiceKeyEnabled="true" latin:hasVoiceKey="false">
                    <key-style latin:code="@integer/key_switch_alpha_symbol" latin:iconPreview="@drawable/sym_keyboard_feedback_123_mic" latin:keyIcon="@drawable/sym_keyboard_123_mic" latin:styleName="toSymbolKeyStyle" latin:parentStyle="functionalKeyStyle" />
                </case>
                <default>
                    <key-style latin:code="@integer/key_switch_alpha_symbol" latin:keyLabel="@string/label_to_symbol_key" latin:styleName="toSymbolKeyStyle" latin:parentStyle="functionalKeyStyle" />
                </default>
            </switch>
            <key-style latin:code="@integer/key_settings" latin:iconPreview="@drawable/sym_keyboard_settings_holo" latin:keyIcon="@drawable/sym_keyboard_settings_holo" latin:styleName="settingsKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_space" latin:iconPreview="@drawable/sym_keyboard_space_holo" latin:keyIcon="@drawable/sym_keyboard_space_holo" latin:styleName="spaceKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_tab" latin:iconPreview="@drawable/sym_keyboard_tab_holo" latin:keyIcon="@drawable/sym_keyboard_tab_holo" latin:styleName="tabKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_voice" latin:iconPreview="@drawable/sym_keyboard_feedback_mic" latin:keyIcon="@drawable/sym_keyboard_mic" latin:styleName="micKeyStyle" latin:parentStyle="settingsPopupStyle" />
            <key-style latin:code="@integer/key_tab" latin:iconPreview="@drawable/sym_keyboard_tab_holo" latin:keyIcon="@drawable/sym_keyboard_tab_holo" latin:styleName="nonSpecialBackgroundTabKeyStyle" />
        </case>
        <case latin:colorScheme="black">
            <key-style latin:code="@integer/key_shift" latin:isSticky="true" latin:iconPreview="@drawable/sym_keyboard_feedback_shift" latin:keyIcon="@drawable/sym_bkeyboard_shift" latin:shiftedIcon="@drawable/sym_bkeyboard_shift_locked" latin:styleName="shiftKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_delete" latin:isRepeatable="true" latin:iconPreview="@drawable/sym_keyboard_feedback_delete" latin:keyIcon="@drawable/sym_bkeyboard_delete" latin:styleName="deleteKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <switch>
                <case latin:voiceKeyEnabled="true" latin:hasVoiceKey="false">
                    <key-style latin:code="@integer/key_switch_alpha_symbol" latin:iconPreview="@drawable/sym_keyboard_feedback_123_mic" latin:keyIcon="@drawable/sym_bkeyboard_123_mic" latin:styleName="toSymbolKeyStyle" latin:parentStyle="functionalKeyStyle" />
                </case>
                <default>
                    <key-style latin:code="@integer/key_switch_alpha_symbol" latin:keyLabel="@string/label_to_symbol_key" latin:styleName="toSymbolKeyStyle" latin:parentStyle="functionalKeyStyle" />
                </default>
            </switch>
            <key-style latin:code="@integer/key_settings" latin:iconPreview="@drawable/sym_keyboard_settings_holo" latin:keyIcon="@drawable/sym_bkeyboard_settings" latin:styleName="settingsKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_space" latin:iconPreview="@drawable/sym_keyboard_feedback_space" latin:keyIcon="@drawable/sym_bkeyboard_space" latin:styleName="spaceKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_tab" latin:iconPreview="@drawable/sym_keyboard_feedback_tab" latin:keyIcon="@drawable/sym_bkeyboard_tab" latin:styleName="tabKeyStyle" latin:parentStyle="functionalKeyStyle" />
            <key-style latin:code="@integer/key_voice" latin:iconPreview="@drawable/sym_keyboard_feedback_mic" latin:keyIcon="@drawable/sym_bkeyboard_mic" latin:styleName="micKeyStyle" latin:parentStyle="settingsPopupStyle" />
            <key-style latin:code="@integer/key_tab" latin:iconPreview="@drawable/sym_keyboard_feedback_tab" latin:keyIcon="@drawable/sym_bkeyboard_tab" latin:styleName="nonSpecialBackgroundTabKeyStyle" />
        </case>
    </switch>
    <switch>
        <case latin:imeAction="actionGo">
            <key-style latin:code="@integer/key_return" latin:keyLabel="@string/label_go_key" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
        <case latin:imeAction="actionNext">
            <key-style latin:code="@integer/key_return" latin:keyLabel="@string/label_next_key" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
        <case latin:imeAction="actionDone">
            <key-style latin:code="@integer/key_return" latin:keyLabel="@string/label_done_key" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
        <case latin:imeAction="actionSend">
            <key-style latin:code="@integer/key_return" latin:keyLabel="@string/label_send_key" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
        <case latin:imeAction="actionSearch">
            <switch>
                <case latin:colorScheme="white">
                    <key-style latin:code="@integer/key_return" latin:iconPreview="@drawable/sym_keyboard_feedback_search" latin:keyIcon="@drawable/sym_keyboard_search" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
                </case>
                <case latin:colorScheme="black">
                    <key-style latin:code="@integer/key_return" latin:iconPreview="@drawable/sym_keyboard_feedback_search" latin:keyIcon="@drawable/sym_bkeyboard_search" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
                </case>
            </switch>
        </case>
        <default>
            <switch>
                <case latin:colorScheme="white">
                    <key-style latin:code="@integer/key_return" latin:iconPreview="@drawable/sym_keyboard_return_holo" latin:keyIcon="@drawable/sym_keyboard_return_holo" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
                </case>
                <case latin:colorScheme="black">
                    <key-style latin:code="@integer/key_return" latin:iconPreview="@drawable/sym_keyboard_feedback_return" latin:keyIcon="@drawable/sym_bkeyboard_return" latin:styleName="returnKeyStyle" latin:parentStyle="functionalKeyStyle" />
                </case>
            </switch>
        </default>
    </switch>
    <key-style latin:code="@integer/key_switch_alpha_symbol" latin:keyLabel="@string/label_to_alpha_key" latin:styleName="toAlphaKeyStyle" latin:parentStyle="functionalKeyStyle" />
    <key-style latin:code="@integer/key_shift" latin:isSticky="true" latin:keyLabel="@string/label_alt_key" latin:styleName="altKeyStyle" latin:parentStyle="functionalKeyStyle" />
    <key-style latin:popupCharacters="@string/alternates_for_smiley" latin:maxPopupKeyboardColumn="5" latin:keyOutputText=":-) " latin:keyLabel=":-)" latin:keyHintIcon="@drawable/hint_popup" latin:styleName="smileyKeyStyle" latin:parentStyle="functionalKeyStyle" />
    <switch>
        <case latin:passwordInput="true">
            <key-style latin:enabled="false" latin:styleName="nonPasswordSymbolKeyStyle" />
            <key-style latin:enabled="false" latin:styleName="nonPasswordFunctionalKeyStyle" latin:parentStyle="functionalKeyStyle" />
        </case>
        <default>
            <key-style latin:enabled="true" latin:styleName="nonPasswordSymbolKeyStyle" />
            <key-style latin:enabled="true" latin:styleName="nonPasswordFunctionalKeyStyle" latin:parentStyle="functionalKeyStyle" />
        </default>
    </switch>
</merge>
