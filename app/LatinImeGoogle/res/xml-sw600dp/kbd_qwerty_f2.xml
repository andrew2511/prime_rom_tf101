<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:f2KeyMode="settings">
            <Key latin:keyWidth="0.0%p" latin:keyEdgeFlags="right" latin:keyStyle="settingsKeyStyle" />
        </case>
        <case latin:f2KeyMode="shortcutIme">
            <switch>
                <case latin:voiceKeyEnabled="true">
                    <Key latin:keyWidth="0.0%p" latin:keyEdgeFlags="right" latin:keyStyle="micKeyStyle" />
                </case>
                <default>
                    <Spacer />
                </default>
            </switch>
        </case>
        <case latin:f2KeyMode="shortcutImeOrSettings">
            <switch>
                <case latin:voiceKeyEnabled="true">
                    <Key latin:keyWidth="0.0%p" latin:keyEdgeFlags="right" latin:keyStyle="micKeyStyle" />
                </case>
                <default>
                    <Key latin:keyWidth="0.0%p" latin:keyEdgeFlags="right" latin:keyStyle="settingsKeyStyle" />
                </default>
            </switch>
        </case>
        <default>
            <Spacer />
        </default>
    </switch>
</merge>
