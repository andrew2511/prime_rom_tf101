<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#3f808080" android:endColor="#7f000000" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:drawable="@drawable/panel_now_playing_progress_bg" />
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff85d20f" android:endColor="#ff466607" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>
