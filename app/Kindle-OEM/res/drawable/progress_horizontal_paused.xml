<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#e61b2a46" android:endColor="#e6144c85" android:angle="270.0" android:centerY="0.25" android:centerColor="#e61e344e" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#20ffd300" android:endColor="#28ffcb00" android:angle="270.0" android:centerY="0.75" android:centerColor="#20ffb600" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff6c86a1" android:endColor="#ff4d5b73" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff445a72" />
            </shape>
        </clip>
    </item>
</layer-list>
